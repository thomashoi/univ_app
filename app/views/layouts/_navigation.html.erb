<header>
  <!-- Dropdown Structure -->
  <ul id="dropdown1" class="dropdown-content">
    <li><%= session_link %></li>
    <% if logged_in? %>
      <li><%= link_to "Profile", current_user %></li>
    <% end %>
    <li class="divider"></li>
    <li><a href="#!">three</a></li>
  </ul>
  <ul id="dropdown2" class="dropdown-content">
    <li><%= session_link %></li>
    <% if logged_in? %>
      <li><%= link_to "Profile", current_user %></li>
    <% end %>
    <li class="divider"></li>
    <li><a href="#!">three</a></li>
  </ul>
  <nav>
    <div class="container">
      <div class="nav-wrapper">
        <%= link_to 'Tech University', root_path, class: "brand-logo" %>
        <a href="#" data-target="mobile-demo" class="sidenav-trigger"><i class="material-icons">menu</i></a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
          <li><%= link_to 'Courses', root_path %></li>
          <% if logged_in? %>
            <li><%= link_to 'Student', students_path %></li>
          <% end %>
          <!-- Dropdown Trigger -->
          <li><a class="dropdown-trigger" href="#!" data-target="dropdown1">Account
                              <i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>
        <ul class="sidenav" id="mobile-demo">
          <li><%= link_to 'Courses', root_path %></li>
          <% if logged_in? %>
            <li><%= link_to 'Student', students_path %></li>
          <% end %>
          <li><a class="dropdown-trigger" href="#!" data-target="dropdown2">Account
                              <i class="material-icons right">arrow_drop_down</i></a></li>
        </ul>
      </div>
    </div>
  </nav>
  <% flash.each do |type, msg| %>
    <div class="container">
      <div class="row sign-up-form">
        <div class="col s12 m12">
          <div class="card-panel red lighten-2" id="fade-out-target">
            <span class="white-text">
              <%= msg %>
            </span>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</header>
